{
  "name": "khsing/world",
  "description": "Provide countries, states, and cities relations database.",
  "keywords": [
    "world",
    "country",
    "city",
    "region"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1|^8.0",
    "laravel/framework": "^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0|^1.4.4",
    "phpunit/phpunit": "^8.5.8|^9.3.3|^9.5.10"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Khsing\\World\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Guixing Bai",
      "email": "khsing.cn@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Khsing\\World\\WorldServiceProvider"
      ]
    }
  }
}
